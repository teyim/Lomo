{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "start": "turbo run start",
    "start:api": "turbo run start --filter=api",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "@repo/db": "workspace:*"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@9.1.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}